{
  "name": "mobx-jsonapi-store",
  "version": "2.0.0-beta.1",
  "description": "JSON API Store for MobX",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "ntsc --outDir ./dist --experimentalDecorators --target es5 --module commonjs --declaration --pretty src/index.ts",
    "test": "NODE_ENV=test nyc mocha",
    "lint": "tslint './src/**/*.ts'",
    "watch": "nodemon -e .ts -i node_modules -i dist -i coverage -x mocha",
    "precommit": "npm run lint && npm test && npm run build && git add dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infinum/mobx-jsonapi-store.git"
  },
  "keywords": [
    "mobx",
    "jsonapi",
    "store",
    "observable"
  ],
  "bugs": {
    "url": "https://github.com/infinum/mobx-jsonapi-store/issues"
  },
  "homepage": "https://github.com/infinum/mobx-jsonapi-store#readme",
  "author": "Infinum <javascript.team@infinum.co>",
  "contributors": [
    {
      "name": "Darko Kukovec",
      "email": "darko@infinum.co"
    }
  ],
  "nyc": {
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "exclude": [
      "src/interfaces/*",
      "test/*"
    ],
    "sourceMap": true,
    "instrument": true,
    "cache": false,
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.4",
    "chai": "^3.5.0",
    "husky": "^0.13.1",
    "mobx": "^3.0.2",
    "mobx-collection-store": "^0.8.0",
    "mocha": "^3.2.0",
    "ntypescript": "^1.201609302242.1",
    "nyc": "^10.1.2",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.2",
    "typescript": "^2.1.5"
  },
  "peerDependencies": {
    "mobx": "^2.6.0 || ^3",
    "mobx-collection-store": "^0.8.0"
  }
}
